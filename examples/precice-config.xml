<?xml version="1.0" encoding="UTF-8" ?>
<precice-configuration>
  <log enabled="0">
    <sink filter="%Severity% > warning" />
  </log>

  <solver-interface dimensions="2">
    <data:scalar name="Pressure" />
    <data:scalar name="CrossSectionLength" />

    <mesh name="Fluid-Nodes-Mesh">
      <use-data name="CrossSectionLength" />
      <use-data name="Pressure" />
    </mesh>

    <mesh name="Adapter-Mesh">
      <use-data name="CrossSectionLength" />
      <use-data name="Pressure" />
    </mesh>

    <participant name="Fluid">
      <use-mesh name="Fluid-Nodes-Mesh" provide="yes" />
      <use-mesh name="Adapter-Mesh" from="Adapter" />
      <write-data name="Pressure" mesh="Fluid-Nodes-Mesh" />
      <read-data name="CrossSectionLength" mesh="Fluid-Nodes-Mesh" />
      <mapping:nearest-neighbor
        direction="read"
        from="Adapter-Mesh"
        to="Fluid-Nodes-Mesh"
        constraint="consistent" />
    </participant>

    <participant name="Adapter">
      <use-mesh name="Adapter-Mesh" provide="yes" />
      <use-mesh name="Fluid-Nodes-Mesh" from="Fluid" />
      <write-data name="CrossSectionLength" mesh="Adapter-Mesh" />
      <read-data name="Pressure" mesh="Adapter-Mesh" />
      <mapping:nearest-neighbor
        direction="read"
        from="Fluid-Nodes-Mesh"
        to="Adapter-Mesh"
        constraint="consistent" />
    </participant>

    <m2n:sockets from="Fluid" to="Adapter" exchange-directory=".."/>

    <coupling-scheme:serial-explicit>
      <participants first="Adapter" second="Fluid" />
      <max-time value="1.0" />
      <time-window-size value="0.1" valid-digits="8" />
      <exchange data="Pressure" mesh="Fluid-Nodes-Mesh" from="Fluid" to="Adapter" initialize="true"/>
      <exchange data="CrossSectionLength" mesh="Adapter-Mesh" from="Adapter" to="Fluid" />

    </coupling-scheme:serial-explicit>
  </solver-interface>
</precice-configuration>
