FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    "(cylinder_wall|top|bottom)"
    {
        type noSlip;
    }
    
    jet1
    {
        type            cylindricalSinusoidalProfile2D;
        origin          ( 0 0 0.005 ); //- arc origin
        radius          0.05;  //- arc radius
        arcCentre         90; ///- arc centre
        arcWidth               10; //- arc angle
        volumetricFlowRate constant 1e-04; 
        value           uniform ( 0 0 0 );
    }

    jet2
    {
        type            cylindricalSinusoidalProfile2D;
        origin          ( 0 0 0.005 ); 
        radius          0.05;  
        arcCentre         270; 
        arcWidth               10; 
        volumetricFlowRate constant -1e-04; 
        value           uniform ( 0 0 0 );
    }

    "(front|back)"
    {
        type            empty;
    }
}


// ************************************************************************* //
