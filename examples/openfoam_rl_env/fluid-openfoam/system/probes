probes
{
    fields ( p U T);
    probeLocations
    (
        (0.1 0.0 0.2)
        (0.3 0.0 0.2)
        (0.5 0.0 0.2)
        (0.7 0.0 0.2)
        (0.9 0.0 0.2)
    );

    #includeEtc "caseDicts/postProcessing/probes/probes.cfg"
}