actions
(
    {
        name    jet1Faces;
        type    faceSet;
        action  new;
        source  arcToFace;
        patch   cylinder_wall;
        origin  ( 0 0 0.005 ); // origin of the cylindrical-arc patch 
        radius  0.05; // radius of the cylindrical-arc patch 
        arcCentre  90; // angle of arc-patch centre
        arcWidth       10; // width of arc-patch
    }

    {
        name    jet2Faces;
        type    faceSet;
        action  new;
        source  arcToFace;
        patch   cylinder_wall;
        origin  ( 0 0 0.005 ); // origin of the cylindrical-arc patch 
        radius  0.05; // radius of the cylindrical-arc patch
        arcCentre  270; // angle of arc-patch centre
        arcWidth       10; // width of arc-patch
    }
);


